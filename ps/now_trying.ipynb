   "source": [
    "# 택배 https://www.acmicpc.net/problem/8980\n",
    "\n",
    "input = open(\"input.txt\").readline\n",
    "\n",
    "n, c = map(int, input().split())\n",
    "m = int(input())\n",
    "box_info = list()\n",
    "for _ in range(m) :\n",
    "    x, y, w = map(int, input() .split())\n",
    "    w = min(c, w)\n",
    "    box_info.append((x, y, w))\n",
    "print(box_info)"
   ]
  }
 ],
